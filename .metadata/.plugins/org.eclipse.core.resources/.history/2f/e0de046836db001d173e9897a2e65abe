package practice2;

class Node{
	int data;
	Node link;
	
}
static Node head;
public class LinkedListSorting {

	public static void main(String[] args) {
		
	}

		private static Node insertEnd(int data) {

			if (head == null) {
				head = new Node();
				head.data = data;
				head.link = null;
			} else {
				Node temp = head;
				while (temp.link != null)
					temp = temp.link;
				Node n = new Node();
				n.data = data;
				temp.link = n;
				n.link = null;
				n = temp = null;
			}
			System.out.println("Element inserted");
		}
}
