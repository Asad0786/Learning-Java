package practice2;

class Node {
	int data;
	Node link;
}

public class SortingLL {
	static Node head = null;

	public static void main(String[] args) {
		head = null;
		insertEnd(10);
		insertEnd(3);
		insertEnd(5);
		insertEnd(1);
		sortList();

	}

	private static void sortList() {
		Node current = head;
		Node next = current;
		if (current.link == null) {
			next = next.link;
			if (current.data > next.data)
				swap(current.data, next.data);
			current = current.link;
		}

	}

	private static void swap(int cData, int nData) {
		cData = cData + nData;
		nData = cData - nData;
		cData = cData - nData;
	}

	private static void insertEnd(int data) {

		if (head == null) {
			head = new Node();
			head.data = data;
			head.link = null;
		} else {
			Node temp = head;
			while (temp.link != null)
				temp = temp.link;
			Node n = new Node();
			n.data = data;
			temp.link = n;
			n.link = null;
			n = temp = null;
		}
		System.out.println("Element inserted");
	}

}
